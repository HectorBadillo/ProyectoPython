
class Contacto:
    def __init__(self, 
                 nombre:str,
                 email:str,
                 no_telefono:str) -> None:
        self.nombre = nombre
        self.email = email
        self.no_telefono = no_telefono
    
    def mostrar_contacto(self):
        print(f"Nombre contacto: {self.nombre}")
        print(f"Mail: {self.email}")
        print(f"Numero de telefono: {self.no_telefono}")
